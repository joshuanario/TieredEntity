@model System.Collections.Generic.List<System.Collections.Generic.List<TieredEntity.Web.Models.ALDutyPosition>>

@{
    ViewData["Title"] = "ALDutyPositions";
}

<h1>ALDutyPositions</h1>

<table>
    <thead>
        <tr><th>Nested Sets on Duty Positions</th></tr>
    </thead>
    <tbody>
        @foreach (var gen in Model)
        {
            <tr>
                @foreach (var item in gen)
                {
                    <td>
                        <div>@Html.DisplayFor(modelItem => item.VertexId)</div>
                        <div>@Html.DisplayFor(modelItem => item.Title)</div>
                        <div>@Html.DisplayFor(modelItem => item.NextId)</div>
                    </td>
                }
            </tr>
        }
    </tbody>
</table>