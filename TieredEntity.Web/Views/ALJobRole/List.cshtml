@model System.Collections.Generic.List<System.Collections.Generic.List<TieredEntity.Web.Models.ALJobRole>>

@{
    ViewData["Title"] = "ALJobRoles";
}

<h1>ALJobRoles</h1>
<h4>Adjacency List on Job Roles</h4>

<table style="table-layout: fixed">
    <tbody>
        @foreach (var gen in Model)
        {
            <tr class="tier-row">
                @foreach (var item in gen)
                {
                    <td style="white-space: nowrap; width: 203px; overflow: auto" class="cell">
                        <span>@Html.DisplayFor(modelItem => item.VertexId)</span>&nbsp;
                        <span>@Html.DisplayFor(modelItem => item.Title)</span>&nbsp;
                        <span>@Html.DisplayFor(modelItem => item.NextId)</span>
                    </td>
                }
            </tr>
        }
    </tbody>
</table>